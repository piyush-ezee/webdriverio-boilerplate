{
  "name": "webdriverio-boilerplate",
  "version": "3.0.0",
  "description": "boilerplate framework for testing using node and webdriverio",
  "main": "wdio.conf.js",
  "engines": {
    "node": ">= 12.16.1"
  },
  "scripts": {
    "test": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js",
    "test:e2e": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite e2e",
    "test:integration": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite integration",
    "test:local": "node_modules/.bin/wdio examples/Amazon/wdio.conf.local.js --suite dev"
  },
  "dependencies": {
    "@wdio/browserstack-service": "^6.6.3",
    "@wdio/cli": "^6.6.6",
    "@wdio/devtools-service": "^6.6.6",
    "@wdio/dot-reporter": "^6.6.6",
    "@wdio/local-runner": "^6.6.6",
    "@wdio/mocha-framework": "^6.6.6",
    "@wdio/selenium-standalone-service": "^6.6.5",
    "@wdio/spec-reporter": "^6.6.6",
    "@wdio/sync": "^6.6.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "chromedriver": "^86.0.0",
    "dotenv": "^8.2.0",
    "mocha": "^8.1.3",
    "selenium-standalone": "^6.20.1",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.6.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonyet/webdriverio-boilerplate.git"
  },
  "author": "jonathan terry",
  "license": "MIT",
  "devDependencies": {
    "@wdio/junit-reporter": "^7.4.2"
  }
}
